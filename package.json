    "gatsby-transformer-json": "^1.0.16",
    "gatsby-transformer-sharp": "^1.6.22",
    "loadjs": "^3.5.2",
    "object-fit-images": "^3.2.3",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
