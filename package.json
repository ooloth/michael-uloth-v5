    "postcss-cli": "^5.0.0",
    "precss": "^3.1.2",
    "purgecss": "^0.20.0"
  },
  "scripts": {
    "watch:postcss": "postcss src/styles/index.css -o src/styles/builds/after-postcss/output.css -w",
    "build:postcss": "postcss src/styles/index.css -o src/styles/builds/after-postcss/output.css",
    "build:purgecss": "purgecss -c ./purgecss.config.js -o src/styles/builds/after-purgecss",
    "dev": "gatsby develop & npm run watch:postcss",
    "build": "npm run build:postcss && npm run build:purgecss && gatsby build"
  }
