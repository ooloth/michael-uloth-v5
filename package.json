{
  "name": "michael-uloth-website",
  "description": "Official website of Toronto-based opera singer and web developer Michael Uloth.",
  "version": "1.0.0",
  "author": "Michael Uloth <hello@michaeluloth.com>",
  "browserslist": [
    ">0.25%",
    "not ie < 11",
    "not op_mini all"
  ],
  "dependencies": {
    "gatsby": "^1.9.255",
    "gatsby-cli": "^1.1.51",
    "gatsby-image": "^1.0.47",
    "gatsby-link": "^1.6.41",
    "gatsby-plugin-netlify": "^1.0.19",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-react-next": "^1.0.11",
    "gatsby-plugin-sharp": "^1.6.43",
    "gatsby-source-filesystem": "^1.5.34",
    "gatsby-transformer-json": "^1.0.16",
    "gatsby-transformer-sharp": "^1.6.23",
    "gatsby-transformer-yaml": "^1.5.17",
    "intersection-observer": "^0.5.0",
    "loadjs": "^3.5.4",
    "object-fit-images": "^3.2.3",
    "react-helmet": "^5.2.0",
    "react-icons": "^2.2.7",
    "react-inlinesvg": "^0.8.1",
    "react-transition-group": "^2.3.1",
    "react-waypoint": "^8.0.1",
    "tailwindcss": "^0.5.2",
    "what-input": "^5.0.5"
  },
  "devDependencies": {
    "autoprefixer": "^8.3.0",
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-plugin-compat": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "gatsby-plugin-sitemap": "^1.2.20",
    "postcss-cli": "^5.0.0",
    "precss": "^3.1.2",
    "purgecss": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-recommended": "^2.1.0",
    "stylelint-no-unsupported-browser-features": "^3.0.0"
  },
  "scripts": {
    "watch:postcss": "postcss src/styles/index.css -o src/styles/builds/after-postcss/output.css -w",
    "build:postcss": "postcss src/styles/index.css -o src/styles/builds/after-postcss/output.css",
    "build:purgecss": "purgecss -c ./purgecss.config.js -o src/styles/builds/after-purgecss",
    "dev": "gatsby develop & npm run watch:postcss",
    "build": "npm run build:postcss && npm run build:purgecss && gatsby build"
  }
}
